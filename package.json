{
  "name": "create-rawfs",
  "version": "1.0.0",
  "description": "Create react SPA with typescript and faker local server.",
  "scripts": {
    "prisma": "prisma",
    "start:front": "webpack --config ./configs/webpack.config.dev.ts --watch",
    "start:server": "nodemon --watch server ./server/server.ts",
    "prestart": "npm run docker:db:up && prisma generate",
    "start": "concurrently \"npm run start:front\" \"npm run start:server\"",
    "docker:db:up": "docker-compose -f docker-compose.db.yml up -d",
    "docker:db:down": "docker-compose -f docker-compose.db.yml down",
    "docker:db:build": "docker-compose -f docker-compose.db.yml build"
  },
  "keywords": [
    "react",
    "server",
    "typescript",
    "create",
    "fake"
  ],
  "author": "Yura Chuchola",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com:SMHFandA/create-rawfs.git"
  },
  "homepage": "https://github.com:SMHFandA/create-rawfs.git#readme",
  "devDependencies": {
    "@faker-js/faker": "^6.0.0",
    "@types/dotenv-webpack": "^7.0.3",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.21",
    "@types/pg": "^8.6.5",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.22",
    "@types/react-router-dom": "^5.3.3",
    "@types/sequelize": "^4.28.11",
    "@types/webpack-hot-middleware": "^2.25.6",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "css-loader": "^6.6.0",
    "dotenv-checker": "^1.1.5",
    "eslint": "^8.10.0",
    "eslint-plugin-react": "^7.29.2",
    "express": "^4.17.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.15",
    "react-router-dom": "^6.2.1",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.7.0",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-middleware": "^5.3.1",
    "webpack-hot-middleware": "^2.25.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@prisma/client": "^3.11.1",
    "@reduxjs/toolkit": "^1.7.2",
    "bulma": "^0.9.4",
    "classnames": "^2.3.1",
    "concurrently": "^7.0.0",
    "dotenv-webpack": "^7.1.0",
    "lodash": "^4.17.21",
    "pg": "^8.7.3",
    "prisma": "^3.11.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "typescript": "^4.5.5"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  }
}
